[user]
	name = PugzAreCute
	email = me@pugzarecute.com
[commit]
	gpgsign = true
[credential]
	credentialStore = secretservice
	helper = /usr/bin/git-credential-manager-core
[credential "https://dev.azure.com"]
	useHttpPath = true
[color]
	ui = auto
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
